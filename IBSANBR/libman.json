{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "cdnjs",
      "library": "jquery@3.3.1",
      "destination": "wwwroot/lib/jquery",
      "files": [
        "jquery.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "noty@3.2.0-beta",
      "destination": "wwwroot/lib/noty/",
      "files": [
        "lib/themes/semanticui.css",
        "lib/noty.min.js",
        "lib/noty.css"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "animate.css@3.7.0",
      "destination": "wwwroot/lib/animate.css/",
      "files": [
        "animate.min.css"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "highcharts@6.1.2",
      "destination": "wwwroot/lib/highcharts/",
      "files": [
        "highcharts.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "jquery.inputmask@3.3.4",
      "destination": "wwwroot/lib/jquery.inputmask/",
      "files": [
        "jquery.inputmask.bundle.min.js"
      ]
    }
  ]
}